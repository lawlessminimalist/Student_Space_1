            </Frame>

            <Frame Grid.Row="2" 
                   Grid.Column="1"
                   BorderColor="#374B90"
                   CornerRadius="7" 
                   Margin="1,1,1,1"
                   HasShadow="False">

            </Frame>

            <Frame Grid.Row="2"  
                   BorderColor="#374B90"
                   CornerRadius="7" 
                   Margin="1,1,1,1"
                   HasShadow="False">

            </Frame>

            <Frame Grid.Row="3"
                   Grid.ColumnSpan="2"  
                   BorderColor="#374B90"
                   CornerRadius="7" 
                   Margin="1,1,1,1"
                   HasShadow="False">

